.nav{
  width: 710rpx;
  height: 330rpx;
  background: #fff;
  margin: 20rpx;
  border-radius: 28rpx;
}
.store{
  height: 85rpx;
 background: linear-gradient(to right, #ffad2c, #ff8f2c);
 border-radius: 28rpx 28rpx 0 0;
}
.store .date{
  display: block;
  float: right;
  height: 85rpx;
  line-height: 85rpx;
  font-size: 26rpx;
  color: #fff;
  margin-right: 62rpx;
}
.store .logo{
  float: left;
  width: 162rpx;
  margin-left: 28rpx;
  height: 85rpx;
}
.store .logo image{
  width: 33rpx;
  height:30rpx; 
  display: block;
  margin: 27rpx 16rpx 27rpx 0;
  float: left;
}
.store .logo text{
  display: block;
  float: left;
  height: 85rpx;
  line-height: 85rpx;
  font-size: 30rpx;
  color: #f6f6f6;
}
.content{
  width: 710rpx;
  height: 247rpx;
}
.content image{
  display: block;
  float: left;
  width: 200rpx;
  height: 200rpx;
  margin: 17rpx 19rpx 30rpx 28rpx;
}
.content text.int{
  width: 436rpx;
  height: 105rpx;
  margin: 26rpx 28rpx 25rpx 0;
  line-height: 35rpx;
  display: block;
  float: left;overflow: hidden;
  font-size: 28rpx;
  color: #3d3d3d;
}
.content view{
  float: left;
  height: 61rpx;
  width: 436rpx;
}
.content view text{
  line-height: 61rpx;
  display: block;
  float: left;
  height: 61rpx;
  font-size: 26rpx;
  color: #646464;
}
.content view text:first-child{
  color: #ff802c;
  margin-right: 23rpx;
}
.content view text:last-child{
  color: #f27e0d;
  float: right;
}
.Courier{
  width: 710rpx;
  height: 100rpx;
  margin: 21rpx 20rpx ;
  background: #fff;
  border-radius: 11rpx;
}
.Courier image{
  width: 45rpx;
  height: 43rpx;
  margin: 32rpx 13rpx 32rpx 28rpx;
  display: block;
  float: left;
}
.Courier text{
    display: block;
  float: left;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #414141;
}
.Courier text.dan{
  float: right;
  font-size: 26rpx;
  margin-right: 28rpx;
}
.progress_b{
  padding: 43rpx 0 83rpx 0;
  width: 710rpx;
  height: auto;
  background: #fff;
  border-radius: 28rpx;
  margin: 0 20rpx;
}
.progress{
  width: 710rpx;
  overflow: hidden;
  height: auto;
}
.progress .left{
  width: 72rpx;
  height: 60rpx;
  float: left;
  margin-right: 12rpx;
  margin-left: 20rpx;
}
.progress .left text{
  display: block;
  line-height: 30rpx;
  font-size: 22rpx;
  color: #999;
}
.progress .center{
  float: left;
  width: 60rpx;
  margin-right: 18rpx;
  height: 158rpx;
}
.progress .center text{
  width: 5rpx;
  height: 98rpx;
  border-left: 2rpx dashed #e8e8e8;
  display: block;
  float: left;
  margin-left: 27rpx;
}
.progress .center view{
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background: #ff9b2c;
  border-radius: 50%;
  font-size: 30rpx;
  color: #fff;
  margin-right: 18rpx;
  float: left;
}
.progress .dress{
  float: left;
  width: 500rpx;
  height: 129rpx;
  margin-top: 7rpx;
  margin-bottom: 20rpx;
}
.progress .dress text{
  display: block;
  width: 500rpx;
}
.progress .dress text:first-child{
  height: 39rpx;
  line-height: 39rpx;
  font-size: 30rpx;
  color: #333333;
}
.progress .dress text:last-child{
  height: 90rpx;
  line-height: 45rpx;
  overflow: hidden;
  font-size: 26rpx;
  color: #999;
}
