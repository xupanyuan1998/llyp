  <image wx:if="{{item.src!=undefined}}" src='{{item.src}}'></image>
  <view class='content'>
    <view class='top'>
      <text  wx:if="{{item.src!=''}}">{{item.lei}}</text>
      <text>ï¿¥{{item.recharge_money}}</text>
    </view>
    <text class='date'>{{item.create_time}}</text>
  </view>
  <text class='zt'>{{item.zt}}</text>
</view>
